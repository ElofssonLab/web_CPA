month = dec,
number = {23},
pages = {6387--96},
title = {{Crystal structure of a 12 ANK repeat stack from human ankyrinR.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=136955\&tool=pmcentrez\&rendertype=abstract},
volume = {21},
