	author = {Uhlen, M. and Oksvold, P. and Fagerberg, L. and Lundberg, E. and Jonasson, K. and Forsberg, M. and Zwahlen, M. and Kampf, C. and Wester, K. and Hober, S. and Wernerus, H. and Bjorling, L. and Ponten, F.},
