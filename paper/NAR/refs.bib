@article {Maixner162,
	author = {Maixner, Frank and Krause-Kyora, Ben and Turaev, Dmitrij and Herbig, Alexander and Hoopmann, Michael R. and Hallows, Janice L. and Kusebauch, Ulrike and Vigl, Eduard Egarter and Malfertheiner, Peter and Megraud, Francis and O{\textquoteright}Sullivan, Niall and Cipollini, Giovanna and Coia, Valentina and Samadelli, Marco and Engstrand, Lars and Linz, Bodo and Moritz, Robert L. and Grimm, Rudolf and Krause, Johannes and Nebel, Almut and Moodley, Yoshan and Rattei, Thomas and Zink, Albert},
	title = {The 5300-year-old Helicobacter pylori genome of the Iceman},
	volume = {351},
	number = {6269},
	pages = {162--165},
	year = {2016},
	doi = {10.1126/science.aad2545},
	publisher = {American Association for the Advancement of Science},
	abstract = {Five thousand years ago in the European Alps, a man was shot by an arrow, then clubbed to death. His body was subsequently mummified by ice until glacier retreat exhumed him in 1991. Subsequently, this ancient corpse has provided a trove of intriguing information about copper-age Europeans. Now, Maixner et al. have identified the human pathogen Helicobacter pylori within the mummy{\textquoteright}s stomach contents. The strain the {\textquotedblleft}Iceman{\textquotedblright} hosted appears to most closely resemble pathogenic Asian strains found today in Central and Southern Asia.Science, this issue p. 162The stomach bacterium Helicobacter pylori is one of the most prevalent human pathogens. It has dispersed globally with its human host, resulting in a distinct phylogeographic pattern that can be used to reconstruct both recent and ancient human migrations. The extant European population of H. pylori is known to be a hybrid between Asian and African bacteria, but there exist different hypotheses about when and where the hybridization took place, reflecting the complex demographic history of Europeans. Here, we present a 5300-year-old H. pylori genome from a European Copper Age glacier mummy. The {\textquotedblleft}Iceman{\textquotedblright} H. pylori is a nearly pure representative of the bacterial population of Asian origin that existed in Europe before hybridization, suggesting that the African population arrived in Europe within the past few thousand years.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/351/6269/162},
	eprint = {http://science.sciencemag.org/content/351/6269/162.full.pdf},
	journal = {Science}
}


